<script>
  
	
  
    // <![CDATA[
    var selectCallback_1 = function(variant, selector) {
	  var product_handle = 'ecoflow-delta-max-solar-generators';
      if (variant) {
		if(variant.title.indexOf("160W") > -1){
		  $('.solar-panel-image-160').show();
		  $('.solar-panel-image-220, .solar-panel-image-400').hide();
		}
		else if(variant.title.indexOf("400W") > -1){
		  $('.solar-panel-image-400').show();
		  $('.solar-panel-image-220, .solar-panel-image-160').hide();
		}
		else if(variant.title.indexOf("220W") > -1){
		  $('.solar-panel-image-220').show();
		  $('.solar-panel-image-400, .solar-panel-image-160').hide();
		}
		else{
		  $('.solar-panel-image-160').show();
		  $('.solar-panel-image-220, .solar-panel-image-400').hide();
		}
		if(variant.option{{qty_option_index}} == 0){
		  $('.product-qty-counter[data-handle="ecoflow-delta-max-solar-generators"]').hide();
		}
		else{
		  console.log('Test' + variant.option{{qty_option_index}});
		  $('.product-qty-counter[data-handle="ecoflow-delta-max-solar-generators"]').show().find('.count').html('X'+variant.option{{qty_option_index}});
		}
		
        if (variant.available) {
          // Selected a valid variant that is available.
          $('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.atc-button').removeAttr('disabled').html('Comprar Ahora');
        } else {
          // Variant is sold out.
          $('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.atc-button').html('Vendido').attr('disabled', 'disabled');
        }
		var money_format = $('.pdp-detail-section').attr('data-money-format');
        // Whether the variant is in stock or not, we can update the price and compare at price.
		$('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.regular-price').html(Shopify.formatMoney(variant.price, money_format).replace('.00',''));
        if ( variant.compare_at_price > variant.price ) {
		  var saving = variant.compare_at_price - variant.price;
          $('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.compare-price-wrapper').html('<div class="compare-price">'+ Shopify.formatMoney(variant.compare_at_price, money_format).replace('.00','') + '</div>'+'<div class="saving-amount">(Ahorrar '+Shopify.formatMoney(saving, money_format).replace('.00','') + ')</div>');
        } else {
          $('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.compare-price-wrapper').html('');
        }
      } else {
        // variant doesn't exist.
        $('.pdp-detail-section[data-handle="'+product_handle+'"]').find('.atc-button').html('Unavailable').attr('disabled', 'disabled');
		$('.product-qty-counter[data-handle="ecoflow-delta-max-solar-generators"]').hide();
      }
    }
    
    jQuery(function($) {
      new Shopify.OptionSelectors("ProductSelect-{{ product.id }}", { product: {{ product | json }} , onVariantSelected: selectCallback_1 });
    });
  
  	
  </script>
