                <style>
                    .block-{{ block.id }}{ background: {{ section.settings.info_background }}; }
                    .block-{{ block.id }} span.product_label_1{ color: {{ block.settings.tag_1_color }}; }
                    .block-{{ block.id }} span.product_label_2{ color: {{ block.settings.tag_2_color }}; }
                    .block-{{ block.id }} .star_product_title p{ color: {{ block.settings.title_color }}; }
                    .block-{{ block.id }} .star_product_desc p{ color: {{ block.settings.description_color }}; }
                    .block-{{ block.id }} .star_product_subtitle p{ color: {{ block.settings.subtitle_color }}; }
                    .block-{{ block.id }} .star_product_price{ color: {{ block.settings.price_color }}; }
                    .block-{{ block.id }} .star_product_compare_price{ color: {{ block.settings.compare_price_color }}; }
                    .block-{{ block.id }} .star_timer_tag{ color: {{ block.settings.timer_tag_color }}; }
                    .block-{{ block.id }} klarna-placement *::part(osm-message), .block-{{ block.id }} klarna-placement *::part(osm-cta){ color: {{ section.settings.klarna_msg_color }}; }
                    .block-{{ block.id }} .discount_tag_wrapper .discount_tag_text, .block-{{ block.id }} .discount_tag_wrapper .discount_tag_text span{ color: {{ section.settings.discount_color }}; }
                    .block-{{ block.id }} .special_price_label{ color: {{ block.settings.special_price_label_color }}; }

                    .block-{{ block.id }} .button_1{
                        {% if section.settings.btn_text_color != blank %}color: {{ section.settings.btn_text_color }};{% endif %}
                        {% if section.settings.btn_bg_color != blank %}background-color: {{ section.settings.btn_bg_color }};{% endif %}
                        {% if section.settings.btn_border_color != blank %}border: 1px solid {{ section.settings.btn_border_color }};{% endif %}
                    }
                    .block-{{ block.id }} .button_1:hover{
                        {% if section.settings.btn_text_color_hover != blank %}color: {{ section.settings.btn_text_color_hover }};{% endif %}
                        {% if section.settings.btn_bg_color_hover != blank %}background-color: {{ section.settings.btn_bg_color_hover }};{% endif %}
                        {% if section.settings.btn_border_color_hover != blank %}border: 1px solid {{ section.settings.btn_border_color_hover }};{% endif %}
                        {% if section.settings.btn_bg_hover_opacity > 0 %}
                        opacity: {{ section.settings.btn_bg_hover_opacity | divided_by: 100.0 }};
                        {% endif %}
                    }

                    .block-{{ block.id }} .button_2{
                        {% if section.settings.btn_2_text_color != blank %}color: {{ section.settings.btn_2_text_color }};{% endif %}
                        {% if section.settings.btn_2_bg_color != blank %}background-color: {{ section.settings.btn_2_bg_color }};{% endif %}
                        {% if section.settings.btn_2_border_color != blank %}border: 1px solid {{ section.settings.btn_2_border_color }};{% endif %}
                    }
                    .block-{{ block.id }} .button_2:hover{
                        {% if section.settings.btn_2_text_color_hover != blank %}color: {{ section.settings.btn_2_text_color_hover }};{% endif %}
                        {% if section.settings.btn_2_bg_color_hover != blank %}background-color: {{ section.settings.btn_2_bg_color_hover }};{% endif %}
                        {% if section.settings.btn_2_border_color_hover != blank %}border: 1px solid {{ section.settings.btn_2_border_color_hover }};{% endif %}
                        {% if section.settings.btn_bg_hover_opacity > 0 %}
                            opacity: {{ section.settings.btn_bg_hover_opacity | divided_by: 100.0 }};
                        {% endif %}
                    }

                    .block-{{ block.id }} .checkout_atc_btn .atc_link{
                        {% if section.settings.checkout_atc_icon_bg_color != blank %}background-color: {{ section.settings.checkout_atc_icon_bg_color }};{% endif %}
                    }
                    .block-{{ block.id }} .checkout_atc_btn .button_1 + .atc_link{
                        {% if section.settings.btn_text_color != blank %}color: {{ section.settings.btn_text_color }};{% endif %}
                        {% if section.settings.btn_border_color != blank %}border: 1px solid {{ section.settings.btn_border_color }};{% endif %}
                    }
                    .block-{{ block.id }} .checkout_atc_btn .button_2 + .atc_link{
                        {% if section.settings.btn_2_text_color != blank %}
                            color: {{ section.settings.btn_2_text_color }};
                        {% endif %}
                        {% if section.settings.btn_2_border_color != blank %}
                            border: 1px solid {{ section.settings.btn_2_border_color }};
                        {% endif %}
                    }
                    .block-{{ block.id }} .checkout_atc_btn .atc_link:hover{
                        {% if section.settings.checkout_atc_icon_bg_color_hover != blank %}
                            background-color: {{ section.settings.checkout_atc_icon_bg_color_hover }};
                        {% endif %}
                        {% if section.settings.checkout_atc_icon_bg_hover_opacity > 0 %}
                            opacity: {{ section.settings.checkout_atc_icon_bg_hover_opacity | divided_by: 100.0 }};
                        {% endif %}
                    }
                    .block-{{ block.id }} .checkout_atc_btn .button_1 + .atc_link:hover{
                        {% if section.settings.btn_text_color_hover != blank %}color: {{ section.settings.btn_text_color_hover }};{% endif %}
                        {% if section.settings.btn_border_color_hover != blank %}border: 1px solid {{ section.settings.btn_border_color_hover }};{% endif %}
                    }
                    .block-{{ block.id }} .checkout_atc_btn .button_2 + .atc_link:hover{
                        {% if section.settings.btn_2_text_color_hover != blank %}
                            color: {{ section.settings.btn_2_text_color_hover }};
                        {% endif %}
                        {% if section.settings.btn_2_border_color_hover != blank %}
                            border: 1px solid {{ section.settings.btn_2_border_color_hover }};
                        {% endif %}
                    }

                    @media screen and (max-width: 749px){
                        .block-{{ block.id }} .star-product-info-wrapper{ background: {{ section.settings.info_background }}; }
                    }
                </style>
            </div>
        {% endfor %}

        </div>
        <div class="star-page-container view_more_wrapper {% if blocks <= number_of_products %}hideDesktop{% endif %} {% if blocks <= number_of_products_m or section.settings.slider_mobile %}hideMobile{% endif %}">
            <div class="view_more_btn" style="--text-color: {{ section.settings.view_more_btn_text_color }}; --border-color: {{ section.settings.view_more_btn_border_color }};">
                <span data-view-more="{{ section.settings.view_more_btn_text | default: 'Show More' }}" data-view-less="{{ section.settings.view_less_btn_text | default: 'Show Less' }}">{{ section.settings.view_more_btn_text | default: 'Show More' }}</span>
                <svg xmlns="http://www.w3.org/2000/svg" width="16" height="17" viewBox="0 0 16 17" fill="none">
                    <path d="M4 6.5L7.5 10.5L11 6.5" stroke="black" stroke-width="1.5" stroke-linecap="round" stroke-linejoin="round"/>
                </svg>
            </div>
        </div>
        {% if section.settings.button_text != blank %}
            <div class="view_more_wrapper product_grid_btn"><a style="color:{{ section.settings.button_text_color }};" href="{{ section.settings.button_url }}">{{ section.settings.button_text }}</a></div>
        {% endif %}
    </div>

    <style>
        .section-{{ section.id }} .star-image{ background: {{ section.settings.image_background }}; }
    </style>
{% endif %}
