{% if section.blocks.size > 0 %}
    <div class="star-page-product-double-v1-section star-page-product-multiple-v1-section section-{{ section.id }}" id="{{ section.settings.section_id }}" style="background-color: {{ section.settings.section_background }};">
        <div class="star-products-container star-page-container {% if section.settings.slider_mobile %}slider-in-mobile{% endif %} per-row-{{section.settings.blocks_per_row}} mobile-per-row-{{section.settings.blocks_per_row_m}}" data-flickity-config='{
            "prevNextButtons": false,
            "wrapAround": false,
            "dragThreshold": 15,
            "cellAlign": "left",
            "pauseAutoPlayOnHover": true,
            "autoPlay": {{ section.settings.round_autoplay_mobile | default: false}},,
            "pageDots": true,
            "adaptiveHeight": false,
            "contain": true,
            "freeScroll": true,
            "watchCSS": true,
            "groupCells": true
          }'>

          {% assign text_padding_left = 1 %}

          {% if section.settings.section_background == section.settings.info_background and section.settings.image_background != '' %}
            {% assign text_padding_left = 0 %}
          {% elsif section.settings.section_background == 'rgba(0,0,0,0)' and section.settings.info_background == '' and section.settings.image_background != '' %}
            {% assign text_padding_left = 0 %}
          {% elsif section.settings.section_background == '' and section.settings.info_background == 'rgba(0,0,0,0)' and section.settings.image_background != '' %}
            {% assign text_padding_left = 0 %}
          {% elsif section.settings.info_background == 'rgba(0,0,0,0)' or section.settings.info_background == '' %}
            {% assign text_padding_left = 0 %}
          {% endif %}

          {% assign blocks = section.blocks.size %}
          {% assign block_rows_d = section.settings.rows_desktop %}
          {% assign block_rows_m = section.settings.rows_mobile %}
          {% assign number_of_products = section.settings.blocks_per_row | times: block_rows_d %}
          {% assign number_of_products_m = section.settings.blocks_per_row_m | times: block_rows_m %}
          {% assign currency_symbol = cart.currency.symbol %}
        {%- comment %}<locksmith:de2f>{% endcomment -%}
          {%- assign locksmith_435e_forloop__size = 0 %}{% for block in section.blocks %}{% capture var %}{% render 'locksmith-variables', scope: 'subject', subject: block.settings.product, subject_parent: section, variable: 'transparent' %}{% endcapture %}{% if var == 'true' %}{% assign locksmith_435e_forloop__size = locksmith_435e_forloop__size | plus: 1 %}{% endif %}{% endfor %}{% assign locksmith_435e_forloop__index = nil -%}
        {%- comment %}</locksmith:de2f>{% endcomment -%}
        {% for block in section.blocks %}
            {%- comment %}<locksmith:45e3>{% endcomment -%}
              {%- capture var %}{% render 'locksmith-variables', scope: 'subject', subject: block.settings.product, subject_parent: section, variable: 'transparent' %}{% endcapture %}{% if var == "true" %}{% if locksmith_435e_forloop__index == nil %}{% assign locksmith_435e_forloop__index = 1 %}{% assign locksmith_435e_forloop__index0 = 0 %}{% else %}{% assign locksmith_435e_forloop__index = locksmith_435e_forloop__index | plus: 1 %}{% assign locksmith_435e_forloop__index0 = locksmith_435e_forloop__index0 | plus: 1 %}{% endif %}{% if locksmith_435e_forloop__index == 1 %}{% assign locksmith_435e_forloop__first = true %}{% else %}{% assign locksmith_435e_forloop__first = false %}{% endif %}{% if locksmith_435e_forloop__index == locksmith_435e_forloop__size %}{% assign locksmith_435e_forloop__last = true %}{% else %}{% assign locksmith_435e_forloop__last = false %}{% endif %}{% assign locksmith_435e_forloop__rindex = locksmith_435e_forloop__size | minus: locksmith_435e_forloop__index | minus: 1 %}{% assign locksmith_435e_forloop__rindex0 = locksmith_435e_forloop__size | minus: locksmith_435e_forloop__index0 | minus: 1 %}{% else %}{% continue %}{% endif -%}
            {%- comment %}</locksmith:45e3>{% endcomment -%}
            {% assign star_product = block.settings.product %}
            {% assign non_member_variant = '' %}
            {% assign non_member_variant_id = block.settings.non_member_variant_id | times: 1 %}
            {% assign member_only_variant = false %}
            {% assign star_gift_product = block.settings.gift_product %}
            {% assign main_product_title = '' %}
            {% if block.settings.product_variant_id == blank %}
                {% assign variant_id = star_product.variants.first.id %}
            {% else %}
                {% assign variant_id = block.settings.product_variant_id | times: 1 %}
            {% endif %}
            {% for variant in star_product.variants %}
                {% if variant.id == variant_id %}
                    {% assign current_variant = variant %}
                    {% assign variant_index = forloop.index %}
                    {% if variant.metafields.custom.for_member_only == true %}
                        {% assign member_only_variant = true %}
                    {% endif %}
                {% endif %}
                {% if variant.id == non_member_variant_id %}
                    {% assign non_member_variant = variant %}
                {% endif %}
            {% endfor %}

            {% if current_variant.metafields.custom.for_member_only == false %}
                {% if non_member_variant_id != '' %}
                    {% for variant in star_product.variants %}
                        {% if variant.id == non_member_variant_id %}
                            {% assign current_variant = variant %}
                        {% endif %}
                    {% endfor %}
                {% else %}
                    {% assign current_variant = star_product.variants[1] %}
                {% endif %}
            {% endif %}

            {% assign main_product_image = block.settings.product_image | default: current_variant.featured_image %}
            {% if block.settings.show_manual_options %}
                {% if block.settings.option_1_text != blank and block.settings.option_1_variant_id != blank and block.settings.option_1_active %}
                    {% assign variant_id = block.settings.option_1_variant_id | times: 1 %}
                    {% assign non_member_variant_id = block.settings.option_1_non_member_variant_id | times: 1 %}
                    {% assign main_product_title = block.settings.option_1_product_title %}
                    {% assign main_product_image = block.settings.option_1_product_image %}
                    {% assign star_product = block.settings.option_1_product %}
                {% endif %}

                {% if block.settings.option_2_text != blank and block.settings.option_2_variant_id != blank and block.settings.option_2_active %}
                    {% assign variant_id = block.settings.option_2_variant_id | times: 1 %}
                    {% assign non_member_variant_id = block.settings.option_2_non_member_variant_id | times: 1 %}
                    {% assign main_product_title = block.settings.option_2_product_title %}
                    {% assign main_product_image = block.settings.option_2_product_image %}
                    {% assign star_product = block.settings.option_2_product %}
                {% endif %}

                {% for variant in star_product.variants %}
                    {% if variant.id == variant_id %}
                        {% assign current_variant = variant %}
                        {% if variant.metafields.custom.for_member_only == true %}
                            {% assign member_only_variant = true %}
                        {% endif %}
                    {% endif %}
                    {% if variant.id == non_member_variant_id %}
                        {% assign non_member_variant = variant %}
                    {% endif %}
                {% endfor %}

                {% if current_variant.metafields.custom.for_member_only == false %}
                    {% if non_member_variant_id != '' %}
                        {% for variant in star_product.variants %}
                            {% if variant.id == non_member_variant_id %}
                                {% assign current_variant = variant %}
                            {% endif %}
                        {% endfor %}
                    {% else %}
                        {% assign current_variant = star_product.variants[1] %}
                    {% endif %}
                {% endif %}
            {% endif %}

            {% capture image_html %}
                <div class="star-product-image-wrapper {% if block.settings.show_hover_image %}hover-image-enable{% endif %}">
                    {% if star_product != blank %}
                        <a {% if section.settings.image_linked %}href="{{ current_variant.url }}"{% endif %}>
                            {% if block.settings.product_image != blank %}
                                <img class="star-image star-product-image" loading="lazy" srcset="{{ block.settings.product_image | image_url }}" />
                            {% elsif main_product_image != blank %}
                                <img class="star-image star-product-image" loading="lazy" srcset="{{ main_product_image | image_url }}" />
                            {% elsif current_variant.featured_image != blank %}
                                <img class="star-image star-product-image" loading="lazy" srcset="{{ current_variant.featured_image | image_url }}" />
                            {% elsif star_product.featured_image != blank %}
                                <img class="star-image star-product-image" loading="lazy" srcset="{{ star_product.featured_image | image_url }}" />
                            {% endif %}

                            {% if block.settings.show_hover_image %}
                                {% if block.settings.hover_product_image != blank %}
                                    <img class="star-image star-product-hover-image" loading="lazy" srcset="{{ block.settings.hover_product_image | img_url: 'small' | replace: '_small', '' }}">
                                {% else %}
                                    <img class="star-image star-product-hover-image" loading="lazy" srcset="{{ current_variant.image | default: star_product.featured_image | img_url: 'small' | replace: '_small', '' }}">
                                {% endif %}
                            {% endif %}
                        </a>
                    {% else %}
                        <a>{{ 'product-1' | placeholder_svg_tag: 'placeholder-svg' }}</a>
                    {% endif %}

                    {% if block.settings.gift_product != blank %}
                        <div class="gift_product_wrapper">
                            <div class="gift_product_image" style="--image-width: {{ block.settings.gift_image_width }}px; --mobile-image-width: {{ block.settings.gift_image_width_m }}px;">
                                {% if block.settings.gift_image != blank %}
                                    <img loading="lazy" srcset="{{ block.settings.gift_image | img_url: 'small' | replace: '_small', '' }}">
                                {% else %}
                                    <img loading="lazy" srcset="{{ star_gift_product.featured_image | img_url: 'small' | replace: '_small', '' }}">
                                {% endif %}
                            </div>
                            <div class="gift_product_info">
                                <div class="gift_product_texts">
                                    {% if block.settings.gift_title != blank %}
                                        <div class="gift_product_title">{{ block.settings.gift_title }}</div>
                                    {% else %}
                                        <div class="gift_product_title">{{ star_gift_product.title }}</div>
                                    {% endif %}
                                    {% if block.settings.gift_subtitle != blank %}
                                        <div class="gift_product_subtitle">{{ block.settings.gift_subtitle }}</div>
                                    {% endif %}
                                </div>
                                <div class="gift_product_prices">
                                  <div class="free_text">{{ block.settings.free_text }}</div>
                                    {% if block.settings.gift_price != blank %}
                                        <div class="gift_product_price">{{ block.settings.gift_price }}</div>
                                    {% else %}
                                        <div class="gift_product_price">{{ star_gift_product.price | money }}</div>
                                    {% endif %}
                                </div>
                            </div>
                        </div>
                    {% endif %}
                </div>
            {% endcapture %}

            <div class="star_product_block block-{{ block.id }} {% if section.settings.image_on_top_m %}mobile--image-top{% endif %} discount_tag_{{ block.settings.tag_alignment }} {% if block.settings.block_visibility == 'pc_only' %}hideMobile{% elsif block.settings.block_visibility == 'mobile_only' %}hideDesktop{% endif %} {% if forloop.index > number_of_products %}blockHideDesktop{% endif %} {% unless section.settings.slider_mobile %}{% if forloop.index > number_of_products_m %}blockHideMobile{% endif %}{% endunless %} {% if text_padding_left == 0 %}no-box-shadow{% endif %}">
                {{ image_html }}

                {% if section.settings.show_discount_tag %}
                    {% if block.settings.discount_text == '' %}
                        {% assign price = 0 %}
                        {% assign compare_price = 0 %}
                        {% if block.settings.price == '' %}
                            {% assign price = current_variant.price | replace: currency_symbol, '' | replace: ',', '' %}
                        {% else %}
                            {% assign price = block.settings.price | replace: currency_symbol, '' | times: 100 %}
                        {% endif %}

                        {% assign compare_price = current_variant.compare_at_price | replace: currency_symbol, '' | replace: ',', '' %}

                        {% assign discount_price = compare_price | minus: price %}
                        {% assign discount_percentage = price | times: 100 | divided_by: compare_price %}
                        {% assign discount_percentage = 100 | minus: discount_percentage | round %}
                    {% endif %}

                    {% if section.settings.discount_badge_type == 'absolute' or section.settings.discount_badge_type == 'percentage'%}
                        {% if discount_price > 0 %}
                        <div class="discount_tag_wrapper">
                            {% if discount_price > 0 or block.settings.discount_text != '' %}{% if section.settings.discount_badge_image != blank %}<img loading="lazy" srcset="{{ section.settings.discount_badge_image | img_url: 'small' | replace: '_small', '' }}" />{% endif %}{% endif %}
                            <div class="discount_tag_text">
                                {% if block.settings.discount_text == '' %}
                                    {% if section.settings.discount_badge_type == 'absolute' and discount_price > 0 %}
                                        {{ section.settings.discount_text | default: 'SAVE' }}<span>{{ discount_price | money_without_trailing_zeros }}</span>
                                    {% elsif section.settings.discount_badge_type == 'percentage' and discount_price > 0 %}
                                        <span>{{ discount_percentage }}%</span>{{ section.settings.discount_text | default: 'OFF' }}
                                    {% endif %}
                                {% else %}
                                    {{ block.settings.discount_text }}
                                {% endif %}
                            </div>
                        </div>
                        {% endif %}
                    {% endif %}
                {% endif %}

                <div class="star-product-info-wrapper text-align-{{ section.settings.text_alignment }} mobile-text-align-{{ section.settings.text_alignment_m }} {% if text_padding_left == 0 %}text-padding-left-zero{% endif %}">
                    <div class="star-product-info">
                        <div class="label_star_ranking">
                            {% if block.settings.tag_1 != blank or block.settings.tag_2 != blank %}
                                <div class="product_labels">
                                    {% if block.settings.tag_1 != blank %}
                                        <span class="product_label_1">{{ block.settings.tag_1 }}</span>
                                    {% endif %}
                                    {% if block.settings.tag_2 != blank %}
                                        <span class="product_label_2">{{ block.settings.tag_2 }}</span>
                                    {% endif %}
                                </div>
                            {% endif %}

                            {% if section.settings.show_star_ranking %}
                                <div class="star_ranking">
                                    <div data-bv-show="rating_summary" data-bv-product-id="{{ star_product.id }}"></div>
                                </div>
                            {% endif %}
                        </div>

                        <div class="star_product_title">
                            {% if section.settings.title_linked %}<a href="{{ current_variant.url }}">{% endif %}
                            {% if main_product_title != blank %}
                                <p>{{ main_product_title }}</p>
                            {% else %}
                                {% if block.settings.title != blank %}
                                    {{ block.settings.title }}
                                {% else %}
                                    <p>{{ star_product.title }}</p>
                                {% endif %}
                            {% endif %}
                            {% if section.settings.title_linked %}</a>{% endif %}
                        </div>

                        {% if block.settings.description != blank %}
                            <div class="star_product_desc">{{ block.settings.description }}</div>
                        {% endif %}

                        <div class="subtitle_price_row">
                            {% if block.settings.subtitle != blank %}
                                <div class="star_product_subtitle">{{ block.settings.subtitle }}</div>
                            {% endif %}

                            {% if block.settings.show_timer_tag %}
                                <div class="star_timer_tag">
                                    <svg width="85" height="72" viewBox="0 0 85 72" fill="none" xmlns="http://www.w3.org/2000/svg">
