  .social-icons__item svg{ fill: {{ section.settings.color_footer_headings }}; }

  .site-footer__linklist-item a {
    color: {{ section.settings.color_footer_links }};
  }
    .site-footer__linklist-item a:hover {
    color: {{ color_footer_links_hover }};
  }

    .cookie-setting {
      color: {{ section.settings.color_footer_links }}!important;
    }
  
  .site-footer {
    color: {{ section.settings.color_footer_text }};
    background-color: {{ section.settings.color_footer_bg }};
  }


  .site-footer__social-icons .social-icons__link:hover {
    color: {{ footer_text_color_hover }};
  }

  .site-footer__policy {
    background-color: transparent;
  }

  .site-footer__policy > li.site-footer__linklist-item {
    padding: 5px 15px 12px 0;
    display: inline-block;
  }
  
  @media screen and (max-width: 1239px){
    .site-footer p:not(.form-message),
    .site-footer small,
    .site-footer__copyright-content a,
    .site-footer__newsletter-error,
    .site-footer__social-icons .social-icons__link,
