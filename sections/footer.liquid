      color: {{ section.settings.color_footer_text_mobile }};
      background-color: {{ section.settings.color_footer_bg_mobile }};
    }
    .social-icons__item svg{ fill: {{ section.settings.color_footer_text_mobile }}; }
    .site-footer__item-inner--newsletter .contact-form .input-group .newsletter__input{ color: {{ section.settings.color_footer_text_mobile }}; border-color: {{ section.settings.color_footer_text_mobile }}; }
    .site-footer__item-inner--newsletter .contact-form .input-group .newsletter__input::placeholder{ color: {{ section.settings.color_footer_text_mobile }}; }
    .site-footer__item-inner--newsletter .contact-form .input-group .newsletter__submit{ background: {{ section.settings.color_footer_text_mobile }}; }
    .site-footer__item-inner--newsletter .contact-form .input-group .newsletter__submit svg path{ fill: #fff; }
  }

  @media screen and (max-width: 749px) {
    .site-footer__policy {
      padding-top: 0;
    }

    .site-footer__policy .site-footer__linklist-item {
      padding: 5px 7px 12px;
    }
  }
</style>


  <script type="module" src="{{ 'dynamic-icon.js' | asset_url }}"></script>

{% schema %}
{
  "name": "t:sections.footer.name",
  "settings": [
    {
      "type": "header",
      "content": "Footer UI Settings"
    },
    {
      "type": "image_picker",
      "id": "footer_bg",
      "label": "t:sections.footer.settings.footer_bg.label"
    },
    {
      "type": "range",
      "id": "bg_opacity",
      "min": 0,
      "max": 100,
      "step": 5,
      "unit": "t:sections.footer.settings.bg_opacity.unit",
      "label": "t:sections.footer.settings.bg_opacity.label",
      "default": 20
    },
    {
      "type": "checkbox",
      "id": "show_copyright_note",
      "label": "t:sections.footer.settings.show_copyright_note.label",
      "default": false
    },
    {
      "type": "richtext",
      "id": "copyright_note",
      "label": "t:sections.footer.settings.copyright_note.label",
      "default": "<p>Copyright &copy; 2023 <a href='/'>Ecoflow</a>. All Rights Reserved.</p>"
    },
    {
      "type": "checkbox",
      "id": "show_payment_icons",
      "label": "t:sections.footer.settings.show_payment_icons.label",
      "default": false
    },
    {
      "type": "header",
      "content": "t:sections.footer.blocks.text.settings.header__3.content",
      "info": "t:sections.footer.blocks.text.settings.header__3.info"
    },
    {
      "type": "checkbox",
      "id": "show_locale_selector",
      "label": "t:sections.footer.blocks.text.settings.show_locale_selector.label",
      "default": false
    },
    {
      "type": "header",
      "content": "t:sections.footer.blocks.text.settings.header__4.content",
      "info": "t:sections.footer.blocks.text.settings.header__4.info"
    },
    {
      "type": "checkbox",
      "id": "show_currency_selector",
      "label": "t:sections.footer.blocks.text.settings.show_currency_selector.label",
      "default": false
    },
    {
      "type": "text",
      "id": "newsletter_title",
      "label": "Newsletter confirmation title",
      "default": "Heading"
    },
    {
      "type": "text",
      "id": "newsletter_submit",
      "label": "Submit button text",
      "default": "Confirm"
    },
    {
      "type": "richtext",
      "id": "newsletter_confirmation",
