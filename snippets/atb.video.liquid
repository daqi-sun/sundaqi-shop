{% comment %}
ATB-SCHEMA{
  "name": "t:sections.video.name",
  "class": "index-section section_with_bg",
  "max_blocks": 3,
  "id": "video",
  "banner": "",
  "icon": "",
  "tags": "",
  "description": "",
  "settings": [
    {
      "type": "text",
      "id": "section_id",
      "label": "Section ID",
      "info": "Enter a unique ID with only letters and numbers, no spaces",
      "default": ""
    },
    {
      "type": "image_picker",
      "id": "atb_image",
      "label": "t:sections.video.settings.image.label",
      "info": ""
    },
    {
      "type": "image_picker",
      "id": "atb_mobile_image",
      "label": "t:sections.video.settings.mobile_image.label",
      "info": ""
    },
    {
      "type": "text",
      "id": "atb_title",
      "label": "t:sections.video.settings.title.label",
      "default": "Heading",
      "info": ""
    },
    {
      "type": "text",
      "id": "atb_subtitle",
      "label": "t:sections.video.settings.subtitle.label",
      "default": "Subheading",
      "info": ""
    },
    {
      "type": "text",
      "id": "blocks",
      "label": "Block ID",
      "info": "Enter a unique ID with only letters and numbers, no spaces",
      "default": "",
      "repeatable": "t:sections.video.blocks.name.label"
    },
    {
      "type": "text",
      "id": "block_heading",
      "label": "t:sections.video.blocks.settings.heading.label",
      "default": "Heading",
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "wysiwyg",
      "id": "block_content",
      "label": "t:sections.video.blocks.settings.content.label",
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "text",
      "id": "block_btn_text",
      "label": "t:sections.video.blocks.settings.button_title.label",
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "text",
      "id": "block_btn_link",
      "label": "t:sections.video.blocks.settings.button_url.label",
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "select",
      "id": "block_button_style",
      "label": "t:sections.video.blocks.settings.button_style.label",
      "default": "primary",
      "options": [
        {
          "label": "t:sections.video.blocks.settings.button_style.options__1.label",
          "value": "primary"
        },
        {
          "label": "t:sections.video.blocks.settings.button_style.options__2.label",
          "value": "secondary"
        },
        {
          "label": "t:sections.video.blocks.settings.button_style.options__3.label",
          "value": "white"
        },
        {
          "label": "t:sections.video.blocks.settings.button_style.options__4.label",
          "value": "transparent_primary"
        },
        {
          "label": "t:sections.video.blocks.settings.button_style.options__5.label",
          "value": "transparent_secondary"
        }
      ],
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "image_picker",
      "id": "block_image",
      "label": "t:sections.video.blocks.settings.image.label",
      "repeatable": "t:sections.video.blocks.name.label",
      "info": ""
    },
    {
      "type": "select",
      "id": "block_video_url_type",
      "label": "t:sections.video.blocks.settings.video_url.label Type",
      "default": "youtube",
      "options": [
        {
          "value": "youtube",
          "label": "Youtube"
        },
        {
          "value": "vimeo",
          "label": "Vimeo"
        }
      ],
      "repeatable": "t:sections.video.blocks.name.label"
    },
    {
      "type": "text",
      "id": "block_video_url_id",
      "label": "YouTube or Vimeo ID",
      "default": "_9VUPq3SxOc",
      "repeatable": "t:sections.video.blocks.name.label"
    },
    {
      "type": "text",
      "id": "block_html5_video_url",
      "label": "t:sections.video.blocks.settings.html5_video_url.label",
      "info": "t:sections.video.blocks.settings.html5_video_url.info",
      "repeatable": "t:sections.video.blocks.name.label"
    }
  ],
  "warnings": [
    "repeating image fields may not be properly handled"
  ]
}ATB-SCHEMA
{% endcomment %}


{%- include 'atb-render' field:'section_id' -%}
{%- assign section_id = val -%}
{%- include 'atb-render' field:'atb_image' -%}
{%- assign atb_image = val -%}
{%- include 'atb-render' field:'atb_mobile_image' -%}
{%- assign atb_mobile_image = val -%}
{%- include 'atb-render' field:'atb_title' -%}
{%- assign atb_title = val -%}
{%- include 'atb-render' field:'atb_subtitle' -%}
{%- assign atb_subtitle = val -%}
{%- include 'atb-render' field:'blocks' -%}
{%- assign blocks = val -%}
{%- include 'atb-render' field:'block_heading' -%}
{%- assign block_heading = val -%}
{%- include 'atb-render' field:'block_content' -%}
{%- assign block_content = val -%}
{%- include 'atb-render' field:'block_btn_text' -%}
{%- assign block_btn_text = val -%}
{%- include 'atb-render' field:'block_btn_link' -%}
{%- assign block_btn_link = val -%}
{%- include 'atb-render' field:'block_button_style' -%}
{%- assign block_button_style = val -%}
{%- include 'atb-render' field:'block_image' -%}
{%- assign block_image = val -%}
{%- include 'atb-render' field:'block_html5_video_url' -%}
{%- assign block_html5_video_url = val -%}
{%- include 'atb-render' field:'block_video_url_id' -%}
{%- assign block_video_url_id = val -%}
{%- include 'atb-render' field:'block_video_url_type' -%}
{%- assign block_video_url_type = val -%}
{%- assign atb_image_key = atb_image| split: '/files/' | last -%}{%- assign atb_image = images[atb_image_key] -%}{%- assign atb_mobile_image_key = atb_mobile_image| split: '/files/' | last -%}{%- assign atb_mobile_image = images[atb_mobile_image_key] -%}{%- assign no_image = images['no_image_comparison'] -%}

<section id="shopify-section-{{section_id}}" class="shopify-section index-section section_with_bg">
<section id="section-{{ section_id }}" data-section-id="{{ section_id }}" data-section-type="video" 
         class='video-section fade-in-animation'>


  <link rel="stylesheet" href="{{ 'photoswipe.css' | asset_url }}" type="text/css">
  <link rel="stylesheet" href="{{ 'default-skin.css' | asset_url }}" type="text/css">

  {% assign image = atb_image %}
  {% assign mobile_image = atb_mobile_image %}
  <div class='{% if image != blank or mobile_image != blank %} section_content {% else %} section_content_without_bg {% endif %}'>
    <div class='page-width'>
      <div class="section-header homepage_subtitle_style_{{settings.homepage_subtitle_color}}" >
        {% if atb_subtitle != blank  %}
       <span class='h5'>{{ atb_subtitle  }}</span>
        {% endif %}

        {% if atb_title != blank %}
        <h2>{{ atb_title | escape }}</h2>
        {% endif %}


      </div> 
      <div class='section-tabs-content'>

        <div class='index-tabs_nav__wrapper {% if blocks.size == 1 %} single_tab {% endif %}'>
          <div class='index-tabs_nav  '> 
            {% for block0 in blocks %}
{%- assign my_image_key0 = block_image[forloop.index0]| split: '/files/' | last -%}
{%- assign my_image0 = images[my_image_key0] -%}
            {% if  block_heading[forloop.index0] != blank %}
            <div class='index-tabs_nav--item {% if forloop.first %} active {% endif %}' data-index="{{forloop.index0}}" data-href='tab_{{block_heading[forloop.index0] | handle }}_{{blocks[forloop.index0]}}' aria-label="{{ block_heading[forloop.index0]}}">
              <h3>{{ block_heading[forloop.index0] }}</h3>
            </div>
            {% endif %}
            {% endfor %}
          </div>
        </div>


        {% for block0 in blocks %}
{%- assign my_image_key0 = block_image[forloop.index0]| split: '/files/' | last -%}
{%- assign my_image0 = images[my_image_key0] -%}
        <div class='index-tabs-content_block {% if forloop.first %} active  {% endif %}' id='tab_{{block_heading[forloop.index0] | handle  }}_{{blocks[forloop.index0]}}'>

          {% assign content = block_content[forloop.index0] %}
          {% assign btn_text = block_btn_text[forloop.index0] %}
          {% assign btn_link = block_btn_link[forloop.index0] %} 
          {% assign video_url = block_video_url_id[forloop.index0] %}
          {% assign image_overlay = my_image0 %}
          {% assign html5_video_url = block_html5_video_url[forloop.index0] %}

          {% if content != blank or  btn_text != blank  %}
          <div class='content_column'>
            
            {% if content != blank %}
            {{content}} 
            {% endif %}

            {% if btn_text != blank %}
            <a class='btn video_tab_link btn--{{block_button_style[forloop.index0] }}' href='{{btn_link}}'> 
              <span>{{ btn_text }}</span>
              {% include 'atb-icon'  with 'tail-right'  %}
            </a>
            {% endif %}
          </div>
          {% endif %}

          {% if video_url != blank or html5_video_url != blank %}
          <div class="VideoWrapper  {% if content == blank and  btn_text == blank %} VideoContentWithoutText {% endif %} ">
            
              {% if image_overlay != blank %}
              <img 
               class="video_image_overlay"
               srcset="{%- if image_overlay.width >= 375 -%}{{ image_overlay | img_url: '375x' }} 375w,{%- endif -%}
                  {%- if image_overlay.width >= 720 -%}{{ image_overlay | img_url: '720x' }} 750w,{%- endif -%}
                  {%- if image_overlay.width >= 1066 -%}{{ image_overlay | img_url: '1066x' }} 1066w,{%- endif -%}
                  {%- if image_overlay.width >= 1500 -%}{{ image_overlay | img_url: '1500x' }} 1500w,{%- endif -%}
                  {%- if image_overlay.width >= 1780 -%}{{ image_overlay | img_url: '1780x' }} 1780w{%- endif -%}"               
               src="{{ image_overlay | img_url: '750x' }}"
               sizes="100vw"
               loading="lazy"
               width="{{ image_overlay.width }}"
               height="{{ image_overlay.height }}"
               alt="{{image_overlay.alt | default:  block_heading[forloop.index0] }}">
              {% else %}


               {%- if video_url.type == 'youtube' -%}
              <img src='https://img.youtube.com/vi/{{video_url.id}}/hqdefault.jpg' class='video_image_overlay' alt='' />
              {% else %}
              <img src='https://i.vimeocdn.com/video/{{video_url.id}}_200x200.webp' class='video_image_overlay' alt='' />
              {% endif %}
            
            {% endif %}
            
            <span class='play_video_btn'>
              {% render 'icon-play'  %}
            </span>

            <div class=' hide VideoModalContent   '>
              {% if video_url != blank and html5_video_url == blank %}
               {%- if video_url.type == 'youtube' -%}
              <iframe id="VideoModal_{{blocks[forloop.index0]}}" src="//www.youtube.com/embed/{{ video_url.id }}?autoplay=0&showinfo=0&controls=1&rel=0&modestbranding=1" frameborder="0" allowfullscreen></iframe>
              {% else %}
              <iframe id="VideoModal_{{blocks[forloop.index0]}}" src="https://player.vimeo.com/video/{{ video_url.id }}" class="js-vimeo" allow="autoplay; encrypted-media" allowfullscreen></iframe>
              {% endif %}
              {% else %}
              {% if html5_video_url != blank %}
              <video controls id="VideoModal_{{blocks[forloop.index0]}}" >
                <source src="{{html5_video_url}}" type="video/mp4">
              </video>
              {% endif %}
              {% endif %}
            </div>

          </div>
          {% endif %}



        </div>
        {% endfor %}
      </div>
    </div>
  </div>
  
   {% if image != blank %}
  <div class='section_bg'>
      
    
        <img 
             class="section_bg_image {% if mobile_image != blank %} small--hide {% endif %}"
             srcset="{%- if image.width >= 375 -%}{{ image | img_url: '375x' }} 375w,{%- endif -%}
                {%- if image.width >= 720 -%}{{ image | img_url: '720x' }} 750w,{%- endif -%}
                {%- if image.width >= 1066 -%}{{ image | img_url: '1066x' }} 1066w,{%- endif -%}
             	{%- if image.width >= 1500 -%}{{ image | img_url: '1500x' }} 1500w,{%- endif -%}
                {%- if image.width >= 1780 -%}{{ image | img_url: '1780x' }} 1780w,{%- endif -%}
                {%- if image.width >= 2000 -%}{{ image | img_url: '2000x' }} 2000w,{%- endif -%}
                {%- if image.width >= 2800 -%}{{ image | img_url: '2800x' }} 2800w{%- endif -%}"
             src="{{ image | img_url: '535x' }}"
             sizes="100vw"
             loading="lazy"
             width="{{ image.width }}"
        	 height="{{ image.height }}"
             alt="{{image.alt | default: atb_title }}">
      
      {% if mobile_image != blank %}
        <img 
             class="section_bg_image mobile_image medium-up--hide"
             srcset="{%- if mobile_image.width >= 375 -%}{{ mobile_image | img_url: '375x' }} 375w,{%- endif -%}
                {%- if mobile_image.width >= 720 -%}{{ mobile_image | img_url: '720x' }} 750w,{%- endif -%}
                {%- if mobile_image.width >= 1066 -%}{{ mobile_image | img_url: '1066x' }} 1066w,{%- endif -%}
             	{%- if mobile_image.width >= 1500 -%}{{ mobile_image | img_url: '1500x' }} 1500w{%- endif -%}"
             src="{{ mobile_image | img_url: '535x' }}"
             sizes="100vw"
             loading="lazy"
             width="{{ mobile_image.width }}"
        	 height="{{ mobile_image.height }}"
             alt="{{mobile_image.alt | default: atb_title  }}">
      {% endif %}
           
   
  </div>
   {% endif %}

</section>




</section>