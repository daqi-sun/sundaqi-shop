      if(Shopify.Checkout.step == 'contact_information') {
        let sectionLogin = $(".section--contact-information").find("a");
        $.each(sectionLogin, function(i,e) {
          var linkText = $(e).html();
          if(linkText !== "" && linkText.toLowerCase().indexOf("log in") != -1) {
              var loginUrl = $(e).attr("href");
              var checkout_url = getURLparams('checkout_url', loginUrl);
              var loginLink = loginUrl+'&return_to='+checkout_url;
              $(e).attr("href", loginLink);
          }
        });
