    <script>
      window.CustomCheckoutOptions = window.CustomCheckoutOptions || {
      targetElementSelector: 'span[data-delivery-pickup-radio-label]',
      stateElementSelector: 'select[name="checkout[shipping_address][province]"]',
      countryElementSelector: 'select[name="checkout[shipping_address][country]"]',
      hideElement: (elm) => elm.classList.add("hiddenInput"),
      showElement: (elm) => elm.classList.remove("hiddenInput"),
      allowedStates: ["CA"],
      allowedCountries: ["United States"]
      }
         CustomCheckoutOptions.pickupAllowed = (stateValue, countryValue) => {
          return CustomCheckoutOptions.allowedStates.some(val => val.toUpperCase() === stateValue?.toUpperCase()) && CustomCheckoutOptions.allowedCountries.some(val => val.toUpperCase() === countryValue?.toUpperCase())
        }

        CustomCheckoutOptions.addEventListeners = (stateElement, countryElement, pickupOption) => {
          stateElement.addEventListener('change', (event) => {
            if(CustomCheckoutOptions.pickupAllowed(event.target.value, countryElement.value) && pickupOption.classList.contains("hiddenInput")) {
              CustomCheckoutOptions.showElement(pickupOption)
            } else if (!CustomCheckoutOptions.pickupAllowed(stateElement.value, event.target.value) && !pickupOption.classList.contains("hiddenInput")){
              CustomCheckoutOptions.hideElement(pickupOption)
            }
          })
          countryElement.addEventListener('change', (event) => {
            if(CustomCheckoutOptions.pickupAllowed() && pickupOption.classList.contains("hiddenInput")) {
              CustomCheckoutOptions.showElement(pickupOption)
            } else if (!CustomCheckoutOptions.pickupAllowed() && !pickupOption.classList.contains("hiddenInput")){
              CustomCheckoutOptions.hideElement(pickupOption)
            }
          })
        }

        CustomCheckoutOptions.setInitalState = (stateElement, countryElement, pickupOption) => {
          console.log(!CustomCheckoutOptions.pickupAllowed(stateElement.value, event.target.value) && !pickupOption.classList.contains("hiddenInput"))
          if(CustomCheckoutOptions.pickupAllowed(stateElement.value, countryElement.value) && pickupOption.classList.contains("hiddenInput")) {
              CustomCheckoutOptions.showElement(pickupOption)
            } else if (!CustomCheckoutOptions.pickupAllowed(stateElement.value, countryElement.value) && !pickupOption.classList.contains("hiddenInput")){
              CustomCheckoutOptions.hideElement(pickupOption)
            }
        }

        CustomCheckoutOptions.init = (e) => {
          if(Shopify.Checkout.step === 'contact_information' ) {
              const pickupOption = document.querySelector(CustomCheckoutOptions.targetElementSelector)?.parentElement.parentElement.parentElement,
                    countryElement = document.querySelector(CustomCheckoutOptions.countryElementSelector),
                    stateElement = document.querySelector(CustomCheckoutOptions.stateElementSelector)
              if(countryElement && stateElement && pickupOption) {
                CustomCheckoutOptions.setInitalState(stateElement, countryElement, pickupOption)
                CustomCheckoutOptions.addEventListeners(stateElement, countryElement, pickupOption)
              }
          }
        }

        document.addEventListener("page:load", CustomCheckoutOptions.init)

        document.addEventListener("page:change", CustomCheckoutOptions.init)
    </script>

