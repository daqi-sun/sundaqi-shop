    "b18f114d-7bdd-48a6-88ae-cfcd44f010c1": {
      "type": "pdp-specs",
      "blocks": {
        "40acd2c3-767b-4bfa-83ea-1651cde16aad": {
          "type": "contents",
          "settings": {
            "title": "BATERÍA",
            "content": "<p><strong>Capacidad: <\/strong>512Wh<\/p><p><strong>Química de las celdas: <\/strong>LFP<\/p><p><strong>Ciclos de vida útil: <\/strong>3000 ciclos a más del 80 % de la capacidad<\/p><p><strong>Temperatura de descarga: <\/strong>Entre -10°C y 45°C<\/p><p><strong>Temperatura de carga: <\/strong>Entre 0 °C y 45 °C<\/p><p><strong>Temperatura de funcionamiento óptima: <\/strong>Entre 20 °C y 30 °C<\/p><p><strong>Temperatura de almacenamiento: <\/strong>Entre -10°C y 45°C (lo mejor es entre 20°C y 30°C)<\/p>"
          }
        },
        "ddb6d485-58d4-4abb-8006-5eece75dc642": {
          "type": "contents",
          "settings": {
            "title": "PUERTOS",
            "content": "<p><strong>Entrada de CA: <\/strong>220-240V 50Hz\/60Hz, 660W Máx<\/p><p><strong>Entrada solar: <\/strong>11-50V, 13A, 110W Máx<\/p><p><strong>Entrada de coche: <\/strong>12V\/24V, 8A, 100W Máx<\/p><p><strong>Entrada\/Salida USB-C: <\/strong>5\/9\/12\/15\/20V, 5A, 100W Máx<\/p><p><strong>Salida de CC: <\/strong>12.6V, 10A\/3A\/3A, 126W Máx<\/p><p><strong>Salida USB-A: <\/strong>5V, 2,4A, 12W Máx<\/p><p><strong>Salida de CA: <\/strong>Onda sinusoidal pura, 500W total (máximo 1000W),230V~50Hz\/60Hz<\/p>"
          }
        },
        "631a8cad-e4e8-4443-bff0-da92f6dda807": {
          "type": "contents",
          "settings": {
            "title": "TIEMPO DE CARGA",
            "content": "<p><strong>Carga de CA: <\/strong>0-80% en 48 minutos 0-100% en 60 minutos (660W Max)<\/p><p><strong>Carga de coche: <\/strong>6<strong> <\/strong>horas<\/p><p><strong>Carga de USB-C: <\/strong>6 horas<\/p><p><strong>Carga solar <\/strong><a href=\"\/products\/river-2-max-160w-portable-solar-panel\" target=\"_blank\" title=\"EcoFlow RIVER 2 Max + Panel Solar Portátil de 160 W\"><strong>(1x 160W panel solar portátil)<\/strong><\/a><strong>: <\/strong>3 horas<\/p>"
          }
        },
        "26759cb4-209a-4700-ae7a-9260d44316f6": {
          "type": "contents",
          "settings": {
            "title": "GENERAL",
            "content": "<p><strong>Peso neto: <\/strong>Aproximadamente 6 kg<\/p><p><strong>Dimensiones: <\/strong>270 x 260 x 196mm<\/p><p><strong>Control mediante aplicación: <\/strong>Wi-Fi, Bluetooth<\/p>"
          }
        }
      },
